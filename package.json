{
  "name": "template",
  "license": "MIT",
  "workspaces": ["core", "web"],
  "scripts": {
    "branch": "git checkout main && git pull && git branch -D $(whoami) ; git checkout -b $(whoami)",
    "b": "bun branch",
    "dev": "bun --hot web/app.ts",
    "start": "bun run core/main.ts",
    "typecheck": "tsc -p tsconfig.json --noEmit",
    "wip": "git add . && git commit -m 'wip' && git push",
    "w": "bun wip"
  },
  "devDependencies": {
    "typescript": "^5.4.0",
    "@template/core": "0.1.2"
  },
  "dependencies": {
    "bun-types": "^1.3.6"
  }
}
